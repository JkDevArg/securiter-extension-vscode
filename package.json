{
  "name": "securiter",
  "displayName": "Securiter",
  "description": "extensión basica para analizar otras extensiones en búsqueda de código malicioso.",
  "version": "0.0.1",
  "publisher": "Joaquin Centurion",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "images/icon.png",
  "author": {
    "name": "Joaquin Centurion",
    "email": "jcenturion@focusit.pe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JkDevArg/securiter-extension-vscode"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.analyzeExtensions",
        "title": "Analyze Extensions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
